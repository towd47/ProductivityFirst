{
    "manifest_version": 3,
    "name": "Productivity First",
    "version": "0.1",
    "options_page": "options/options.html",
    "action": {
        "default_popup": "popup/popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "tabs"
    ]
}